<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Category5">

    <Button
        android:id="@+id/ReturnToRecipes5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="returnToRecipes"
        android:text="Back"
        app:backgroundTint="#5D96DD"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.015"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <Button
        android:id="@+id/EggBiryaniBtn"
        android:layout_width="133dp"
        android:layout_height="47dp"
        android:onClick="loadEggBiryani"
        android:text="Egg Biryani"
        app:backgroundTint="#5D96DD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.377" />

    <Button
        android:id="@+id/TamarindRiceBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="loadTamarindRice"
        android:text="Tamarind Rice"
        app:backgroundTint="#5D96DD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/VegBiryaniBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="loadVegetableBiryani"
        android:text="Vegetable Biryani"
        app:backgroundTint="#5D96DD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.617" />

    <Button
        android:id="@+id/SteamedRiceBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="loadSteamedRice"
        android:text="Steamed Rice"
        app:backgroundTint="#5D96DD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.745" />

    <Button
        android:id="@+id/ChapathiBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="loadChapathi"
        android:text="Chapathi"
        app:backgroundTint="#5D96DD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.857" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rice/Chapathi"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/EggBiryaniBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>